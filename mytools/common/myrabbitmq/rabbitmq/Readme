#经过测试可以直接使用
docker run --name some-app --link some-rabbit:rabbit -d application-that-uses-rabbitmq

docker run -d -e RABBITMQ_NODENAME=my-rabbit --name some-rabbit -p 8080:15672 rabbitmq_base:latest
docker run -d -e RABBITMQ_NODENAME=my-rabbit --name some-rabbit myrabbitmq_base:latest

